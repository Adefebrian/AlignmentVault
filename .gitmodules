[submodule "lib/forge-std"]
	# Submodule for Forge standard library
	path = lib/forge-std
	url = https://github.com/foundry-rs/forge-std

[submodule "lib/solady"]
	# Submodule for Solady library
	path = lib/solady
	url = https://github.com/vectorized/solady

[submodule "lib/openzeppelin-contracts-v5"]
	# Submodule for OpenZeppelin contracts version 5
	path = lib/openzeppelin-contracts-v5
	url = https://github.com/openzeppelin/openzeppelin-contracts

[submodule "lib/nftx-protocol-v3"]
	# Submodule for NFTX protocol version 3
	path = lib/nftx-protocol-v3
	url = https://github.com/Zodomo/nftx-protocol-v3
[submodule "lib/delegate-registry"]
	path = lib/delegate-registry
	url = https://github.com/delegatexyz/delegate-registry
